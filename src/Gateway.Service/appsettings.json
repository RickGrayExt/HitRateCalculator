{
  "ReverseProxy": {
    "Routes": [
      { "RouteId":"r1","ClusterId":"orch","Match":{"Path":"/runs/{**catch}"},"Transforms":[{"PathPattern":"/runs/{**catch}"}]},
      { "RouteId":"r2","ClusterId":"eff","Match":{"Path":"/results/{**catch}"},"Transforms":[{"PathPattern":"/results/{**catch}"}]}
    ],
    "Clusters": {
      "orch": { "Destinations": { "d1": { "Address": "http://orchestrator:8080" } } },
      "eff":  { "Destinations": { "d1": { "Address": "http://eff:8080" } } }
    }
  }
}
